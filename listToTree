function listToTree(list,pid) {
  list.forEach(item => {
    let parentId = item.parentId
    if(parentId !== pid) {
      list.forEach(item2 => {
        if(parentId === item2.id) {
          item2.children = item2.children || []
          item2.children.push(item)
        }
      })
    }
  })
  return list.filter(item => item.id === pid)
}
